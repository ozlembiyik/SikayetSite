    <link rel="stylesheet" type="text/css" href="~/Content/SiteSablon1/plugins/OwlCarousel2-2.3.4/owl.carousel.css">
    <link rel="stylesheet" type="text/css" href="~/Content/SiteSablon1/plugins/OwlCarousel2-2.3.4/owl.theme.default.css">
    <link rel="stylesheet" type="text/css" href="~/Content/SiteSablon1/plugins/OwlCarousel2-2.3.4/animate.css">
    <link rel="stylesheet" type="text/css" href="~/Content/SiteSablon1/styles/main_styles.css">
    <link rel="stylesheet" type="text/css" href="~/Content/SiteSablon1/styles/responsive.css">
